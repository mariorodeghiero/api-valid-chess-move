{
  "env": {
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier", "import"],
  "rules": {
    "prettier/prettier": ["error"],
    "global-require": "off",
    "no-param-reassign": "off",
    "consistent-return": "off",
    "arrow-body-style": "off",
    "no-underscore-dangle": "off",
    "no-console": "off"
  }
}
